# =============================================================================
# JobRunner Environment Variables
# =============================================================================
# Copy this file to .env (already gitignored)

# -----------------------------------------------------------------------------
# PostgreSQL (used by Docker Compose)
# -----------------------------------------------------------------------------
POSTGRES_USER=dagster
POSTGRES_PASSWORD=dagster
POSTGRES_DB=dagster

# -----------------------------------------------------------------------------
# Dagster Configuration (for jr CLI)
# -----------------------------------------------------------------------------
DAGSTER_WEBSERVER_HOST=localhost
DAGSTER_WEBSERVER_PORT=3000

# -----------------------------------------------------------------------------
# LLM API Keys (Optional - for code generation)
# -----------------------------------------------------------------------------
# OPENAI_API_KEY=sk-...
# ANTHROPIC_API_KEY=sk-ant-...
# GOOGLE_API_KEY=...
